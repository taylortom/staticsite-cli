    "pages": {
        "home": {
            "title": {
                "text": "Home",
                "show": false
            },
            "subDir": false
        },
        "work": {
            "title": {
                "text": "Work",
                "show": true
            },
            "subDir": true
        },
        "cv": {
            "title": {
                "text": "CV",
                "show": true
            },
            "subDir": true
        },
        "blog": {
            "title": {
                "text": "The electric word",
                "show": true
